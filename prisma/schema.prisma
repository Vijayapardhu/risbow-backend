  @@index([isActive, effectiveFrom, effectiveTo])
  @@index([minOrderPaise])
  @@index([maxOrderPaise])
  @@index([setByAdminId])
}

model ReferralRewardGrant {
  id                    String             @id @default(cuid())
  orderId               String             @unique
  inviterUserId         String
  inviteeUserId         String
