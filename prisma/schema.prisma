  placedAt       DateTime      @default(now())
  settledAt      DateTime?
  result         String?
  payout         Int?
  createdAt      DateTime      @default(now())
  updatedAt      DateTime      @updatedAt
  user           User          @relation(fields: [userId], references: [id])
  ledgerEntries  LedgerEntry[]

  @@index([userId])
  @@index([status])
