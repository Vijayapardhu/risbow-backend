  updatedAt   DateTime        @updatedAt
  metadata    Json?
  vendor      Vendor          @relation(fields: [vendorId], references: [id], onDelete: Cascade)

  @@index([startDate, endDate])
  @@index([status])
  @@index([type])
  @@index([vendorId])
  @@index([vendorId, status])
}

model Wallet {
  id        String        @id @default(cuid())
  userId    String        @unique
